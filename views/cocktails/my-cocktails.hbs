<h1>Hey, here is a list of your cocktails</h1>

<form> 
 <input id="back-button" type="button" value="Go back!" onclick="history.back()"> 
</form>

<div class="cocktail-list-container">
    {{#each cocktails}}
    <div class="cocktail-container">
        <div class="imgAndTitle">
        <a href="/users/my-cocktails/{{id}}"> <img src="{{imgUrl}}" class="cocktaimImg-list" alt="Cocktail Image"> </a>
            <h3>{{name}}</h3>
        </div>
    </div>
    <hr>
    {{/each}}
</div>

